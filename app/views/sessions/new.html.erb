<h1>Log in</h1>

<%= form_for(:session, url: login_path) do |f| %>
	
	<%= f.label :name %> <br>
	<%= f.text_field :name %> <br>

	<%= f.label :email %> <br>
	<%= f.email_field :email %> <br>

	<%= f.label :password %> <br>
	<%= f.password_field :password %> <br>
	<br>
	<%= f.submit "Log in" %>
<% end -%>

<p>New User? <%= link_to "Sign up now!", signup_path %></p>